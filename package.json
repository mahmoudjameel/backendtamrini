{
  "name": "tamreny-api",
  "version": "1.0.0",
  "description": "Api for tamreny app",
  "main": "index.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run client\" \"npm run server\"",
