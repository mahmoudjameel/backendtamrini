    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "google-auth-library": "^7.6.2",
    "google-map-react": "^2.1.9",
    "google-maps-react": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "mongoose-auto-increment": "^5.0.1",
    "nodemon": "^2.0.6",
